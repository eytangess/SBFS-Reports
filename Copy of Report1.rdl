  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>hc_operating_budget_Data</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Department">
            <Value>=Parameters!Department.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Department, FundCenter, Employees, BudgetYear + CHAR(13)+CHAR(10) + Version AS BudgetYear
FROM     vSBFS_SummaryData
WHERE  (DepartmentID = @Department) AND ((VersionID = 3) AND (BudgetYearID BETWEEN 1 AND 10)) OR  ((DepartmentID = @Department)   AND(BudgetYearID = 11))
ORDER BY DepartmentID, BudgetYear</CommandText>
      </Query>
      <Fields>
        <Field Name="Department">
          <DataField>Department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FundCenter">
          <DataField>FundCenter</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Employees">
          <DataField>Employees</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BudgetYear">
          <DataField>BudgetYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
